#include <iostream>
#include <vector>
#include <algorithm>

using namespace std;

int main() {
    int size;
    cin >> size;

    vector<int> input_vec(size);

    for (int i = 0; i < size; ++i) {
        cin >> input_vec[i];
    }

    vector<int> result_vec;

    for (int current_value : input_vec) {
        if (result_vec.empty() || current_value != result_vec.back()) {
            result_vec.push_back(current_value);
        }
    }

    for (int i = 0; i < result_vec.size(); ++i) {
        cout << result_vec[i] << (i == result_vec.size() - 1 ? "" : " ");
    }
    cout << endl;

    return 0;
}
