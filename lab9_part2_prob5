#include <iostream>
#include <vector>
using namespace std;
int main() {
    vector<int> a;
    vector<int> b;
    vector<int> c;
    int n,x;
    cout<<"Enter number of elements: ";
    cin>>n;
    for (int i = 0; i < n; i++) {
        cin>>x;
        a.push_back(x);
    }
    for (int i = 0; i < n; i++) {
        cin>>x;
        b.push_back(x);
    }
    for (int i = 0; i < n; i++) {
        x=a[i]+b[i];
        c.push_back(x);
    }
    for (int i = 0; i < n; i++) {
        cout<<c[i]<<" ";
    }
}
