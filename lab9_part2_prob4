#include <iostream>
#include <vector>
#include <algorithm>
using namespace std;

int main() {
    vector<int> v;
    int n,count=0,x;
    cout<<"Enter the size of vector: ";
    cin>>n;
    cout<<"Enter the elements of vector: ";
    for(int i=0;i<n;i++) {
        cin>>x;
        v.push_back(x);
    }
    sort(v.begin(),v.end());
    for (int i=0;i<n-1;i++) {
        if(v[i]!=v[i+1]) {
            count++;
        }
    }
    count++;
    cout<<"Unique="<<count;
    return 0;
}
