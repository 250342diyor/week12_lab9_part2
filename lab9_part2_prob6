#include <iostream>
#include <vector>

using namespace std;

int main() {
    int n;
    cin >> n;

    if (n <= 0) {
        cout << "Sorted" << endl;
        return 0;
    }

    vector<int> vec(n);
    bool sorted = true;

    for (int i = 0; i < n; ++i) {
        cin >> vec[i];
    }

    for (int i = 0; i < n - 1; ++i) {
        if (vec[i] > vec[i + 1]) {
            sorted = false;
            break;
        }
    }

    if (sorted) {
        cout << "Sorted" << endl;
    } else {
        cout << "Not Sorted" << endl;
    }

    return 0;
}
