#include <iostream>
#include <vector>

using namespace std;

int main() {
    int r, c;
    cin >> r >> c;

    if (r <= 0 || c <= 0) {
        return 0;
    }

    vector<vector<int>> matrixA(r, vector<int>(c));
    vector<vector<int>> matrixB(r, vector<int>(c));
    vector<vector<int>> matrixSum(r, vector<int>(c));

    for (int i = 0; i < r; ++i) {
        for (int j = 0; j < c; ++j) {
            cin >> matrixA[i][j];
        }
    }

    for (int i = 0; i < r; ++i) {
        for (int j = 0; j < c; ++j) {
            cin >> matrixB[i][j];
        }
    }

    for (int i = 0; i < r; ++i) {
        for (int j = 0; j < c; ++j) {
            matrixSum[i][j] = matrixA[i][j] + matrixB[i][j];
        }
    }

    for (int i = 0; i < r; ++i) {
        for (int j = 0; j < c; ++j) {
            cout << matrixSum[i][j] << (j == c - 1 ? "" : " ");
        }
        cout << endl;
    }

    return 0;
}


This program reads the dimensions, inputs the elements for both matrices, computes their sum element-wise, and prints the resulting sum matrix.
